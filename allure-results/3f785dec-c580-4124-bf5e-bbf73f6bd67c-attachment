# Test source

```ts
   1 | import { Locator, Page } from '@playwright/test';
   2 | import { HeaderElement } from 'src/elements/header.element';
   3 | import { notebookLinkSelector } from 'src/elements/main-page.element';
   4 |
   5 | export class MainPage {
   6 |     private baseUrl = 'https://rozetka.com.ua/ua/';
   7 |
   8 |     private readonly headerElement: HeaderElement;
   9 |
  10 |     public constructor(private readonly page: Page) {
  11 |         this.headerElement = new HeaderElement(this.page.locator('body'));
  12 |     }
  13 |     public get header(): HeaderElement {
  14 |         return this.headerElement;
  15 |     }
  16 |     public get notebookLink(): Locator {
  17 |         return this.page.locator(notebookLinkSelector);
  18 |     }
  19 |     public async goto(): Promise<void> {
  20 |         await this.page.goto(this.baseUrl);
  21 |         await this.header.clickMenuButton();
  22 |         await this.notebookLink.waitFor();
  23 |     }
  24 |
  25 |     public async navigateToNotebooks(): Promise<void> {
> 26 |         await this.notebookLink.click();
     |                                 ^ Error: locator.click: Test timeout of 30000ms exceeded.
  27 |         await this.header.searchFor('Notebook');
  28 |     }
  29 | }
