# Test source

```ts
   1 | import { Locator, Page } from '@playwright/test';
   2 | import { PriceFilterElement } from 'src/elements/price-filter.element';
   3 |
   4 | export class ComputersCategoryPageWithFilters {
   5 |     private baseUrl = 'https://rozetka.com.ua/ua/computers-notebooks/c80253/';
   6 |     private readonly priceFilter: PriceFilterElement;
   7 |     private notebookCategorySelector = '//a[@title="Ноутбуки" and contains(@class, "tile-cats__heading")]';
   8 |     private computersPageTitleSelector = '//h1';
   9 |     private notebookPageTitleSelector = '//h1[text()="Ноутбуки"]';
  10 |     private catalogSelectionSelector = '[class="catalog-selection"]';
  11 |
  12 |     public constructor(private readonly page: Page) {
  13 |         this.priceFilter = new PriceFilterElement(page.locator('rz-filter-slider'));
  14 |     }
  15 |
  16 |     public get notebookCategory(): Locator {
  17 |         return this.page.locator(this.notebookCategorySelector);
  18 |     }
  19 |     public get computersCategoryPageTitle(): Locator {
  20 |         return this.page.locator(this.computersPageTitleSelector);
  21 |     }
  22 |     public get catalogSelection(): Locator {
  23 |         return this.page.locator(this.catalogSelectionSelector);
  24 |     }
  25 |
  26 |     public async goto(): Promise<void> {
  27 |         await this.page.goto(this.baseUrl);
  28 |         await this.computersCategoryPageTitle.waitFor();
  29 |     }
  30 |
  31 |     public async navigateToNotebooks(): Promise<void> {
  32 |         await this.notebookCategory.click();
  33 |         await this.page.locator(this.notebookPageTitleSelector).waitFor();
  34 |     }
  35 |     public async setPriceRange(min: string, max: string): Promise<void> {
  36 |         await this.priceFilter.setRange(min, max);
  37 |         await this.catalogSelection.waitFor();
  38 |       }
  39 |     
  40 |   public async getFilteredGoods(): Promise<void> {
  41 |     await this.priceFilter.apply();
  42 |     await this.catalogSelection.waitFor();
  43 |   }
  44 |     public async filterByPrice(min: string, max: string): Promise<void> {
  45 |         await this.priceFilter.setRange(min, max);
> 46 |         await this.catalogSelection.waitFor();
     |                                     ^ Error: locator.waitFor: Test timeout of 30000ms exceeded.
  47 |     }
  48 | }
